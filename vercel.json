{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "cd vibe-kit-docs && pnpm install && pnpm run build",
  "outputDirectory": "vibe-kit-docs/.next",
  "installCommand": "cd vibe-kit-docs && pnpm install",
  "framework": "nextjs",
  "ignoreCommand": "git diff HEAD^ HEAD --quiet ./vibe-kit-docs/",
  "devCommand": "cd vibe-kit-docs && pnpm run dev"
}
